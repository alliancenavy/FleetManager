<Window x:Class="Client.NewShip"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="Choose Hull" Height="111" Width="300" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="Button_OK" Content="OK" HorizontalAlignment="Left" Margin="164,45,0,0" VerticalAlignment="Top" Width="57" Height="25" Background="#FF316838" Foreground="White" Click="Button_OK_Click"/>
        <Button x:Name="Button_Cancel" Content="Cancel" HorizontalAlignment="Left" Margin="226,45,0,0" VerticalAlignment="Top" Width="57" Height="25" Background="#FF316838" Foreground="White" Click="Button_Cancel_Click"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="273" ItemsSource="{Binding Path=hullList}"/>

    </Grid>
</Window>
